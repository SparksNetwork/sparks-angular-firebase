{
  "name": "client",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "use": "cross-var firebase use %ANGULAR_ENV%",
    "stage": "cross-env-shell ANGULAR_ENV=staging \"npm run preload && npm run build && npm run deploy\" && npm run use",
    "stage:client": "cross-env-shell ANGULAR_ENV=staging \"npm run build:client && npm run deploy:client\" && npm run use",
    "stage:server": "cross-env-shell ANGULAR_ENV=staging \"npm run build:server && npm run deploy:server\" && npm run use",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cross-var ng build --target=production --env=%ANGULAR_ENV%",
    "build:server": "tsc -p tsconfig.json",
    "build:ssr": "ngc -p tsconfig.ssr.json",
    "start": "concurrently \"npm run start:client\" \"npm run start:server\"",
    "start:client": "cross-var ng serve --env=%ANGULAR_ENV%",
    "start:server": "watch \"ts-node ./server/src/index.ts\" ./server/src",
    "preload": "ts-node ./bin/load-test-data.ts",
    "deploy": "npm run use && firebase deploy --only hosting,functions",
    "deploy:client": "npm run use && firebase deploy --only hosting",
    "deploy:server": "npm run use && firebase deploy --only functions",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "cross-var ng e2e --env=%ANGULAR_ENV%"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.4.4",
    "@angular/common": "^4.4.4",
    "@angular/compiler": "^4.4.4",
    "@angular/core": "^4.4.4",
    "@angular/forms": "^4.4.4",
    "@angular/http": "^4.4.4",
    "@angular/platform-browser": "^4.4.4",
    "@angular/platform-browser-dynamic": "^4.4.4",
    "@angular/platform-server": "^4.4.4",
    "@angular/router": "^4.4.4",
    "angular2-moment": "^1.6.0",
    "angularfire2": "5.0.0-rc.1",
    "body-parser": "^1.17.2",
    "bootstrap": "^3.3.7",
    "class-transformer": "^0.1.8",
    "class-transformer-validator": "^0.4.0",
    "class-validator": "^0.7.2",
    "core-js": "^2.4.1",
    "cors": "^2.8.4",
    "enhanced-resolve": "3.4.1",
    "express": "^4.16.1",
    "file-saver": "^1.3.3",
    "firebase": "4.5.0",
    "firebase-admin": "5.4.1",
    "firebase-functions": "0.7.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "moment": "^2.18.1",
    "ngx-bootstrap": "^1.8.1",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.4.1",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "^1.4.4",
    "@angular/compiler-cli": "^4.4.4",
    "@angular/language-service": "^4.4.4",
    "@types/jasmine": "~2.6.0",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~8.0.32",
    "codelyzer": "~3.2.0",
    "concurrently": "^3.5.0",
    "cross-env": "^5.0.5",
    "cross-var": "^1.0.3",
    "firebase-functions-local": "^3.0.3",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jsonschema": "^1.1.1",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "protractor-beautiful-reporter": "^0.5.0",
    "ts-node": "~3.3.0",
    "tslint": "~5.7.0",
    "typescript": "~2.5.3",
    "watch": "^1.0.2"
  }
}
