{
  "name": "client",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "use": "cross-var firebase use %ANGULAR_ENV%",
    "stage": "cross-env-shell ANGULAR_ENV=staging \"npm run preload && npm run build && npm run deploy\" && npm run use",
    "stage:client": "cross-env-shell ANGULAR_ENV=staging \"npm run build:client && npm run deploy:client\" && npm run use",
    "stage:server": "cross-env-shell ANGULAR_ENV=staging \"npm run build:server && npm run deploy:server\" && npm run use",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cross-var ng build --target=production --env=%ANGULAR_ENV%",
    "build:server": "tsc -p tsconfig.json",
    "build:ssr": "ngc -p tsconfig.ssr.json",
    "start": "concurrently \"npm run start:client\" \"npm run start:server\"",
    "start:client": "cross-var ng serve --env=%ANGULAR_ENV%",
    "start:server": "watch \"ts-node ./server/src/index.ts\" ./server/src",
    "preload": "ts-node ./bin/load-test-data.ts",
    "deploy": "npm run use && firebase deploy --only hosting,functions",
    "deploy:client": "npm run use && firebase deploy --only hosting",
    "deploy:server": "npm run use && firebase deploy --only functions",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "cross-var ng e2e --env=%ANGULAR_ENV%",
    "e2e:focus": "cross-var ng e2e --serve=false --config protractor.focus.conf.js --env=%ANGULAR_ENV%"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.0.1",
    "@angular/common": "5.0.1",
    "@angular/compiler": "5.0.1",
    "@angular/core": "5.0.1",
    "@angular/forms": "5.0.1",
    "@angular/http": "5.0.1",
    "@angular/platform-browser": "5.0.1",
    "@angular/platform-browser-dynamic": "5.0.1",
    "@angular/platform-server": "5.0.1",
    "@angular/router": "5.0.1",
    "@types/winston": "^2.3.6",
    "angularfire2": "^5.0.0-rc.3",
    "body-parser": "^1.17.2",
    "bourbon": "^4.3.4",
    "class-transformer": "^0.1.8",
    "class-transformer-validator": "^0.4.0",
    "class-validator": "^0.7.2",
    "core-js": "^2.4.1",
    "cors": "^2.8.4",
    "express": "^4.16.1",
    "express-winston": "^2.4.0",
    "firebase": "4.6.2",
    "firebase-admin": "5.5.0",
    "firebase-functions": "0.7.3",
    "font-awesome": "^4.7.0",
    "ng2-semantic-ui": "^0.9.6",
    "reflect-metadata": "^0.1.10",
    "rxjs": "5.5.2",
    "winston": "^2.4.0",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "^1.4.4",
    "@angular/compiler-cli": "5.0.1",
    "@angular/language-service": "5.0.1",
    "@types/jasmine": "~2.6.0",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~8.0.32",
    "codelyzer": "4.0.1",
    "concurrently": "^3.5.0",
    "cross-env": "^5.0.5",
    "cross-var": "^1.0.3",
    "cucumber": "^3.0.4",
    "firebase-functions-local": "^3.0.3",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jsonschema": "^1.1.1",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "multiple-cucumber-html-reporter": "^1.1.0",
    "protractor": "~5.1.2",
    "protractor-beautiful-reporter": "^0.5.0",
    "protractor-cucumber-framework": "^4.0.8",
    "protractor-multiple-cucumber-html-reporter-plugin": "^1.2.3",
    "ramda": "^0.25.0",
    "sleep-promise": "^2.0.0",
    "ts-node": "~3.3.0",
    "tslint": "~5.7.0",
    "typescript": "2.4.2",
    "watch": "^1.0.2"
  }
}
