{
  "rules": {
    ".read": "true",
    ".write": "auth != null",
    "opp": {
      ".indexOn": ["projectKey"]
    },
    "contrib": {
      ".indexOn": ["oppKey"]
    },
    "benefit": {
      ".indexOn": ["oppKey"]
    },
    "oppAllowedTeam": {
      ".indexOn": ["oppKey"]
    },
    "application": {
      ".indexOn": ["profileKey", "projectProfileKey"]
    },
    "applicationTeam":{
      ".indexOn": ["appKey"]
    },
    "shift":{
      ".indexOn": ["teamKey"]
    },
    "applicationShift": {
      ".indexOn": ["appKey"]
    }
  }
}